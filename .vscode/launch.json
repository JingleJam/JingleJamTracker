    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Website",
            "type": "f5anything",
            "request": "launch",
            "command": "wrangler pages dev ./website --kv=JINGLE_JAM_DATA=6a08d74af525475baefa19a93f0ba136 --do TILTIFY_CACHE=TiltifyData@tiltify-cache --do GRAPH_DATA=GraphData@tiltify-cache",
            "terminalName": "Website",
            "terminalIndex": 1,
            "showTerminal": true
        },
        {
            "name": "Debug API",
            "type": "f5anything",
            "request": "launch",
            "command": "cd ${workspaceFolder}/workers/tiltify-cache/tiltify-cache/ ; wrangler dev --name tiltify-cache",
            "terminalName": "API",
            "terminalIndex": 2,
            "showTerminal": true
        },
        {
            "name": "Add Local KV's",
            "type": "f5anything",
            "request": "launch",
            "command": "wrangler kv:bulk put ${workspaceFolder}/workers/tiltify-cache/tiltify-cache/kv_setup.json --namespace-id=6a08d74af525475baefa19a93f0ba136 --preview --local ; cd ${workspaceFolder}/workers/tiltify-cache/tiltify-cache/ ; wrangler kv:bulk put ./kv_setup.json --namespace-id=6a08d74af525475baefa19a93f0ba136 --preview --local",
            "terminalName": "Other",
            "terminalIndex": 0,
            "showTerminal": true
        },
        {
            "name": "Deploy DEV API",
            "type": "f5anything",
            "request": "launch",
            "command": "cd ${workspaceFolder}/workers/tiltify-cache/tiltify-cache/ ; wrangler deploy --env=development",
            "terminalName": "Other",
            "terminalIndex": 0,
            "showTerminal": true
        },
        {
            "name": "Deploy Production API",
            "type": "f5anything",
            "request": "launch",
            "command": "cd ${workspaceFolder}/workers/tiltify-cache/tiltify-cache/ ; wrangler deploy",
            "terminalName": "Other",
            "terminalIndex": 0,
            "showTerminal": true
        }
    ]
}